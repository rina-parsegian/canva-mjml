      <mj-column>
        <mj-divider border-width="1px" border-style="solid" border-color="#cbd0d6" padding="0"/>
      </mj-column>
    </mj-section>

    <!-- address info -->
    <mj-section padding="30px 30px 10px" background-color="#f2f2f2">
      <mj-column>
        <mj-text font-size="12px" font-weight="normal" font-family="arial" padding="0" line-height="1.6" color="#0d1217" align="center">
          This advertisement is made for you with love from Canva<br>
          Canva Pty Ltd, 110 Kippax St, NSW 2010, Australia. ABN 80 158 929 938<br>
          If you need assistance, get in touch with us via our Help Center.
        </mj-text>
      </mj-column>
    </mj-section>

    <!-- subscribe info -->
    <mj-section padding="10px 30px" background-color="#f2f2f2">
      <mj-column>
        <mj-text font-size="12px" font-weight="normal" font-family="arial" padding="0" line-height="1.6" color="#0d1217" align="center">
          <a href="https://www.canva.com" class="message-info">visit Canva.com</a>
        </mj-text>
        <mj-text font-size="12px" font-weight="normal" font-family="arial" padding="0" line-height="1.6" color="#0d1217" align="center">
          <a href="https://www.canva.com" class="message-info">Unsubscribe</a>
        </mj-text>
        <mj-text font-size="12px" font-weight="normal" font-family="arial" padding="0" line-height="1.6" color="#0d1217" align="center">
          <a href="https://www.canva.com" class="message-info">Message Preferences</a>
        </mj-text>
      </mj-column>
    </mj-section>

    <!-- social icons -->
    <mj-section padding="0 30px 0">
      <mj-group css-class="icons">
        <mj-column>
          <mj-image src="https://i.postimg.cc/SKbMZZ3b/facebook.png" alt="facebook icon" href="https://www.facebook.com/canva" width="24px" padding="0"/>
        </mj-column>
        <mj-column>
          <mj-image src="https://i.postimg.cc/zX9bBtJH/twitter.png" alt="twitter icon" href="https://www.twitter.com/canva" width="24px" padding="0"/>
        </mj-column>
        <mj-column>
          <mj-image src="https://i.postimg.cc/mD81gTDT/pinterest.png" alt="pinterest icon" href="https://www.pinterest.com/canva" width="24px" padding="0"/>
        </mj-column>
        <mj-column>
          <mj-image src="https://i.postimg.cc/J0FyjKZg/instagram.png" alt="instagram icon" href="https://www.instagram.com/canva" width="24px" padding="0"/>
        </mj-column>
      </mj-group>
    </mj-section>


  </mj-body>
</mjml>